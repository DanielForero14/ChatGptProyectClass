{
  "expo": {
    "name": "MiProyectoExpo",
    "slug": "MiProyectoExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true  
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/loadingsplashcreen.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "/assets/images/loadingsplashcreen.jpg"
    },
    "splash": {
      "image": "./assets/images/loadingsplashcreen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#202123"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
